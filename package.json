    "eslint": "*",
    "eslint-config-aegir-standalone": "*",
    "eslint-config-oclif": "*",
    "mkgs-tool": "^0.1.9",
    "mocha": "*",
    "nyc": "*",
    "parcel": "^1.12.4",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "pug": "^3.0.0",
    "babel-polyfill": "^6.26.0",
